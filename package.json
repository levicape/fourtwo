    "main": "module/index.ts",
    "types": "module/index.d.ts",
    "engines": {
        "node": ">=20.0.0"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/levicape/ubiquitous-guide.git"
    },
    "publishConfig": {
      "registry": "https://npm.pkg.github.com/"
    },
    "bin": "./module/cli/bin.js",
    "version": "0.0.0-local.0",
    "type": "module",
    "scripts": {
        "prelint": "npx @biomejs/biome format ./src ./canary",
        "lint": "npx @biomejs/biome check ./src ./canary",
        "lint:format": "npx @biomejs/biome format --write ./src ./canary",
        "lint:write": "npx @biomejs/biome check --write ./src ./canary",
        "lint:unsafe": "npx @biomejs/biome check --write --unsafe ./src ./canary",
        "clean": "rm -rf ./module && rm -rf ./build",
        "clean:dependencies": "rm -rf ./node_modules",
        "build": "npx tsc",
        "pretest": "npm run clean && npm run build",
        "test": "echo 'ok'",
        "test:unit": "node --experimental-strip-types --experimental-test-module-mocks  --test  \"module/**/*.test.js\"",
        "test:canary": "node --experimental-strip-types canary/basic.ts",
        "clean:pack": "rm -rf ./atokos.repopack",
        "pack": "npx clean:pack && npx repopack --output ./atokos.repopack"
    },
    "files": [
        "src",
        "module"
    ],
    "peerDependencies": {
        "typescript": "^5.7.2"
    },
    "devDependencies": {
        "@types/folder-hash": "^4.0.4",
        "@types/node": "^22.10.1",
        "@types/unzipper": "^0.10.10",
        "expect": "^29.7.0"
    },
    "dependencies": {
