        "@pulumi/awsx": "^2.19.0",
        "@pulumi/command": "^1.0.1",
        "@pulumi/docker": "^4.5.7",
        "@pulumi/docker-build": "^0.0.6",
        "@pulumi/kubernetes": "^4.18.3",
        "@pulumi/pulumi": "^3.142.0",
        "@pulumi/random": "^4.16.7",
        "@pulumi/synced-folder": "^0.0.9",
        "@pulumiverse/time": "^0.1.0",
        "@stricli/core": "^1.1.0",
        "@types/module-alias": "^2.0.4",
        "folder-hash": "^4.0.4",
        "ksuid": "^3.0.0",
        "nanoid": "^5.0.9",
        "nexe": "4.0.0-rc.6",
        "slugid": "^5.0.1",
        "unzipper": "^0.12.3",
        "yaml": "^2.6.1"
    },
